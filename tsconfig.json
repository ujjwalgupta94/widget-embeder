{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "inlineSources": true,
        // "out": "output/jsstore.js",
        "target": "es5",
        "preserveConstEnums": true,
        "declaration": true,
        "module": "es6",
        "outDir": "../build/ts",
        "lib": [
            "es2015",
            "dom",
            "webworker"
        ],
        "moduleResolution": "Node"
    },
    "exclude": [
        "node_namespaces",
        "build"
    ]
}